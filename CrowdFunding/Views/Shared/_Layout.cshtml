        .box > .icon {
            text-align: center;
            position: relative;
        }

            .box > .icon > .image {
                position: relative;
                z-index: 2;
                margin: auto;
                width: 88px;
                height: 88px;
                border: 1px solid white;
                border-color: #42ccae;
                line-height: 88px;
                border-radius: 50%;
                background: #F0F0F0;
                vertical-align: middle;
            }

            .box > .icon:hover > .image {
                background: #42ccae;
            }

            .box > .icon > .image > i {
                font-size: 36px !important;
                color: #42ccae !important;
            }

            .box > .icon:hover > .image > i {
                color: white !important;
            }

            .box > .icon > .info {
                margin-top: -24px;
                background: rgba(0, 0, 0, 0.04);
                border: 1px solid #e0e0e0;
                padding: 15px 0 10px 0;
            }

            .box > .icon:hover > .info {
                background: rgba(0, 0, 0, 0.04);
                border-color: #e0e0e0;
                
            }

            .box > .icon > .info > h2.title {
